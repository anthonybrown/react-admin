{
	"ecmaFeatures": {
		"jsx": true
	},
	"env": {
		"browser": true,
		"node": true,
		"jquery": true
	},
	"sourceType": "module",
	"rules": {
		"quotes": 0,
		"no-trailing-spaces": 1,
		"no-underscore-dangle": 0,
		"no-alert": 0,
		"no-multi-spaces": 0,
		"no-lone-blocks": 0,
		"no-unused-vars": 0
	},
	"globals": {
		"jQuery": true,
		"$": true
	}
}
